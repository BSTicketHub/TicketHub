@model TicketHubApp.ViewModels.StoreViewModel


@using (Html.BeginForm())
{
    <div class="container">
        <div class="row">
            <div class="col-12">
                @Html.ValidationSummary(true, "", new { @class = "text-danger" })
                <div class="upLoadImg bg-white p-3 shadow">
                    <form action="/somewhere/to/upload" enctype="multipart/form-data">

                        <input id="upLoadImg" type="file" onchange="readURL(this)" targetID="preview_progressbarTW_img" accept="image/gif, image/jpeg, image/png" />

                        <img id="preview_progressbarTW_img" src="#" />
                    </form>
                </div>
                <div class="description bg-white my-3 p-3 shadow">
                    <form class="d-flex flex-wrap">
                        <div class="form-group col-12">
                            <label for="formGroupExampleInput">商品名稱</label>
                            @*<input type="text" class="form-control form-control-sm" id="formGroupExampleInput" placeholder="請輸入商品名稱">*@
                            @Html.EditorFor(model => model.Title, new { htmlAttributes = new { @class = "form-control form-control-sm", @placeholder = "請輸入商品名稱" } })
                            @Html.ValidationMessageFor(model => model.Title, "", new { @class = "text-danger" })
                        </div>
                        <div class="form-group col-12">
                            <label for="formGroupExampleInput">票券類別</label>
                            <input type="text" class="form-control form-control-sm" id="dropdownCategoryButton" data-toggle="dropdown" placeholder="請輸入商品名稱">
                            <ul id="category" class="dropdown-menu w-100" aria-labelledby="dropdownCategoryButton">
                                <li class="dropdown-item w-100">123</li>
                                <li class="dropdown-item w-100">456</li>
                            </ul>
                        </div>
                        <div class="form-group col-4">
                            <label for="formGroupExampleInput">商品定價</label>
                            @*<input type="text" class="form-control form-control-sm" id="formGroupExampleInput" placeholder="請輸入商品原價">*@
                            @Html.EditorFor(model => model.OriginalPrice, new { htmlAttributes = new { @class = "form-control form-control-sm", @placeholder = "請輸入商品原價" } })
                            @Html.ValidationMessageFor(model => model.OriginalPrice, "", new { @class = "text-danger" })

                        </div>
                        <div class="form-group col-4">
                            <label for="formGroupExampleInput">商品售價</label>
                            @*<input type="text" class="form-control form-control-sm" id="formGroupExampleInput" placeholder="請輸入商品售價">*@
                            @Html.EditorFor(model => model.DiscountPrice, new { htmlAttributes = new { @class = "form-control form-control-sm", @placeholder = "請輸入商品原價" } })
                            @Html.ValidationMessageFor(model => model.DiscountPrice, "", new { @class = "text-danger" })

                        </div>
                        <div class="form-group col-4">
                            <label for="formGroupExampleInput">販售數量</label>
                            @*<input type="text" class="form-control form-control-sm" id="formGroupExampleInput" placeholder="請輸入商品數量">*@
                            @Html.EditorFor(model => model.Amount, new { htmlAttributes = new { @class = "form-control form-control-sm", @placeholder = "請輸入商品原價" } })
                            @Html.ValidationMessageFor(model => model.Amount, "", new { @class = "text-danger" })
                        </div>
                        <div class="form-group col-6">
                            <label for="datetimepicker-released" class="d-block">上架日期</label>
                            <input type="datetime-local" class="w-100 form-control form-control-sm" id="meeting-time" name="meeting-time" value="">
                        </div>
                        <div class="form-group col-6">
                            <label for="datetimepicker-released" class="d-block">下架日期</label>
                            <input type="datetime-local" class="w-100 form-control form-control-sm" id="meeting-time" name="meeting-time" value="">
                        </div>
                    </form>
                </div>
                <div class="description bg-white my-3 p-3 shadow">
                    <div class="form-group col-12">
                        <label for="exampleFormControlTextarea1">商品描述</label>
                        <textarea class="form-control" id="exampleFormControlTextarea1" rows="8"></textarea>
                    </div>
                    <div class="col-12 d-flex">
                        <button type="button" onclick="showModal()" class="ml-auto d-block" data-toggle="modal" data-target="#exampleModalCenter">
                            預覽
                        </button>
                        <button class="ml-2 d-block">新增</button>
                    </div>
                    <!-- Button trigger modal -->
                    <!-- Modal -->
                    <div class="modal fade" id="exampleModalCenter" tabindex="-1" role="dialog" aria-labelledby="exampleModalCenterTitle" aria-hidden="true">
                        <div class="modal-dialog modal-dialog-centered modal-lg" role="document">
                            <div class="modal-content">
                                <div class="modal-header">
                                    <h5 class="modal-title" id="exampleModalLongTitle">票券預覽</h5>
                                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                        <span aria-hidden="true">&times;</span>
                                    </button>
                                </div>
                                <div class="modal-body">
                                    <div id="showDialog" class="row" style="height:250px;">
                                        <div class="col-4 border">
                                            @*<img id="showImg" src="#" />*@
                                            <img id="preview_progressbarTW_img" src="data:image/jpeg;base64,">
                                        </div>
                                        <div class="col-8 border">
                                            <h6>im title im title im title im title</h6>
                                            <p>information information information information information informationinformation information information</p>
                                            <p>台北</p>
                                            <p>9999</p>
                                            <p>1000</p>
                                        </div>
                                    </div>
                                </div>
                                <div class="modal-footer">
                                    <button type="button" class="btn btn-sm btn-secondary" data-dismiss="modal">Close</button>
                                </div>
                            </div>
                        </div>
                    </div>

                </div>
            </div>
        </div>
    </div>

}


@section endJS{
    <script>
        // set upload Image
        function readURL(input) {
            if (input.files && input.files[0]) {
                var imageTagID = input.getAttribute("targetID");
                var reader = new FileReader();
                reader.onload = function (e) {
                    var img = document.getElementById(imageTagID);
                    img.setAttribute("src", e.target.result)
                }
                reader.readAsDataURL(input.files[0]);
            }
        }

        // 預覽功能
        function showModal() {
            let img = document.getElementById("showImg");
            let title = document.querySelector("#showDialog div:nth-child(2)");
            let imageFile = document.getElementById("upLoadImg").files[0];
            console.log(imageFile);
            console.log(title);
            img.setAttribute("src", imageFile.result);
            title.innerText = "123123123123";
        }


        // category
        let categoryList = ["中式", "泰式", "西式"];
        let category = document.getElementById("category");
        createCategoryElements();

        function createCategoryElements() {
            category.innerHTML = "";
            for (let i = 0; i < categoryList.length; i++) {
                let li = document.createElement("li");
                li.classList.add("dropdown-item", "w-100");
                li.innerHTML = categoryList[i];
                category.appendChild(li);
            }
        }

    </script>
}


@section topCSS{
    <style>
        .upLoadImg {
            height: 200px;
        }

        .description {
            font-size: 14px;
        }

        .form-control:focus {
            box-shadow: none;
            border: 2px solid #26bcc7;
        }

        .form-control:focus {
            color: #26bcc7;
        }

        button {
            background-color: #2a94db;
            color: #fff;
            border: 1px solid #2a94db;
            border-radius: 2px;
            padding: 3px 10px;
        }

            button:hover {
                background-color: #fff;
                color: #2a94db;
            }

            button:focus {
                outline: none;
            }

            button:active {
                background-color: #2a94db;
                color: #fff;
            }
    </style>
}